ame":null,"original":{"line":139,"column":2},"generated":{"line":1,"column":1953}},{"source":"faq.css","name":null,"original":{"line":140,"column":0},"generated":{"line":1,"column":1954}}],"sources":{"faq.css":"/*=============== GOOGLE FONTS ===============*/\r\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600&display=swap\");\r\n/*=============== VARIABLES CSS ===============*/\r\n:root {\r\n  /*========== Colors ==========*/\r\n  --hue-color: 225;\r\n  --first-color: hsl(var(--hue-color), 48%, 35%);\r\n  --title-color: #ffffff;\r\n  --text-color: #ffffff;\r\n  --body-color: hsl(var(--hue-color), 49%, 98%);\r\n  --container-color: #fff;\r\n  /*========== Font and typography ==========*/\r\n  --body-font: \"Poppins\", sans-serif;\r\n  --normal-font-size: 0.938rem;\r\n  --small-font-size: 0.813rem;\r\n  --smaller-font-size: 0.75rem;\r\n}\r\n\r\n@media screen and (min-width: 968px) {\r\n  :root {\r\n    --normal-font-size: 1rem;\r\n    --small-font-size: 0.875rem;\r\n    --smaller-font-size: 0.813rem;\r\n  }\r\n}\r\n\r\n/*=============== BASE ===============*/\r\n\r\n/*=============== LAYOUT ===============*/\r\n/* .container {\r\n  max-width: 968px;\r\n  margin-left: 1rem;\r\n  margin-right: 1rem;\r\n} */\r\n\r\n/*=============== ACCORDION ===============*/\r\n\r\n.accordion {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n}\r\n\r\n.accordion__container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  background-color: #ffc23c;\r\n  border-radius: 0.5rem;\r\n  box-shadow: 0 12px 32px rgba(51, 51, 51, 0.1);\r\n  margin: 5px;\r\n  border-right: 4px solid white;\r\n}\r\n\r\n.accordion__header {\r\n  display: flex;\r\n  width: 100%;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  line-height: 1rem;\r\n}\r\n\r\n.accordion__icon {\r\n  font-size: 1.5rem;\r\n  height: max-content;\r\n  color: var(--title-color);\r\n  transition: 0.3s;\r\n}\r\n\r\n.accordion__title {\r\n  font-size: var(--small-font-size);\r\n  color: var(--title-color);\r\n  font-weight: 400;\r\n  margin-left: 1rem;\r\n  transition: 0.2s;\r\n}\r\n\r\n.accordion__item {\r\n  box-shadow: 0 2px 6px rgba(38, 38, 38, 0.1);\r\n  background-color: #222222;\r\n  border-radius: 0.25rem;\r\n  position: relative;\r\n  transition: all 0.25s ease-in-out;\r\n  border-left: 4px solid white;\r\n  margin: 1rem;\r\n  border-radius: 10px;\r\n}\r\n\r\n.accordion__content {\r\n  overflow: hidden;\r\n  height: 0;\r\n  transition: all 0.25s ease;\r\n}\r\n\r\n.accordion__description {\r\n  padding: 0 1.25rem 1.25rem 3rem;\r\n  font-size: var(--smaller-font-size);\r\n  line-height: 1.2rem;\r\n  letter-spacing: 1px;\r\n  width: 80%;\r\n}\r\n\r\n/*Rotate icon and add font weight to titles*/\r\n.accordion-open .accordion__icon {\r\n  transform: rotate(45deg);\r\n}\r\n\r\n.accordion-open .accordion__title {\r\n  font-weight: 600;\r\n}\r\n\r\n/*=============== MEDIA QUERIES ===============*/\r\n/* For medium devices */\r\n@media screen and (min-width: 576px) {\r\n  .accordion__container {\r\n    width: 65%;\r\n    padding: 2.5rem;\r\n    justify-self: center;\r\n    border-radius: 0.75rem;\r\n  }\r\n  .accordion__header {\r\n    padding: 1.5rem;\r\n  }\r\n  .accordion__title {\r\n    padding-right: 3.5rem;\r\n  }\r\n  .accordion__description {\r\n    padding: 0 4rem 1.25rem 3.5rem;\r\n  }\r\n}\r\n\r\n/* For large devices */\r\n@media screen and (min-width: 968px) {\r\n  .container {\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2556a8f56f6ca919b2b26206c5bc81d5","cacheData":{"env":{}}}